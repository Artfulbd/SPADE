tempDir=tmp
constantsSource=spade.reporter.audit.LinuxConstants cfg/spade.reporter.audit.LinuxConstants.config
#set 'reportingIntervalSeconds' to 0 to not report anything
reportingIntervalSeconds=120
spadeAuditBridge=bin/spadeAuditBridge
deleteModule=bin/deleteModule
#Comma-separated names of processes to ignore (case-sensitive). Only for live-audit.
ignoreProcesses=
#Comma-separated names of processes whose immediate children are to be ignored (case-sensitive). Only for live-audit.
ignoreParentProcesses=

# Set to true if need to control explicitly
# If value not set then set to 'true' if the modules exist
#localEndpoints=
#handleLocalEndpoints=
# Main kernel module
kernelModuleMain=lib/kernel-modules/netio.ko
kernelModuleController=lib/kernel-modules/netio_controller.ko
harden=false
kernelModuleDeleteBinary=bin/deleteModule

